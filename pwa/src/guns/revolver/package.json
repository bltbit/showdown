{
  "name": "revolver",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "scripts": {
    "base64": "node b64encode.js",
    "image:generate": "imagemin ./images/originals/revolver-cylinder-resized.png > ./build/cylinder.png",
    "audio:generate": "npm run audio:generate:sprites && npm run audio:generate:typings",
    "audio:generate:sprites": "audiosprite --format howler2 -g 1 --output build/audio ./sounds/export/*.wav",
    "audio:generate:typings": "node genAudioSpriteTypes.js > build/SpriteNames.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "audiosprite": "^0.7.2",
    "image-data-uri": "^2.0.1"
  },
  "devDependencies": {
    "glob": "^7.1.6",
    "imagemin-cli": "^6.0.0",
    "patch-package": "^6.2.2"
  }
}
